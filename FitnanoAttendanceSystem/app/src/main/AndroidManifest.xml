<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="demo.fitnanoattendancesystem">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NewUser" />
        <activity
            android:name=".Management_user_page"
            android:label="@string/title_activity_management_user_page"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".for_teachers"
            android:label="@string/title_activity_for_teachers"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".user_parents"
            android:label="@string/title_activity_user_parents"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>